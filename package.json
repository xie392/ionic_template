{
    "name": "ionic_template",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "npm run copy:sql:wasm && vite --host --mode development",
        "build": "npm run copy:sql:wasm && tsc && vite build",
        "preview": "vite preview",
        "test.e2e": "cypress run",
        "test.unit": "vitest",
        "lint": "eslint",
        "format": "prettier --write ./src",
        "translate": "node node_modules/vue-auto-translate/server.js",
        "copy:sql:wasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm public/assets",
        "postinstall": "node ./scripts/modify-typeorm.cjs",
        "build:android": "npm run build && npx cap sync android",
        "build:ios": "npm run build && npx cap sync ios"
    },
    "dependencies": {
        "@capacitor-community/sqlite": "^5.7.2",
        "@capacitor/android": "6.0.0",
        "@capacitor/app": "6.0.0",
        "@capacitor/core": "6.0.0",
        "@capacitor/haptics": "6.0.0",
        "@capacitor/ios": "6.0.0",
        "@capacitor/keyboard": "6.0.0",
        "@capacitor/status-bar": "6.0.0",
        "@ionic/react": "^8.0.0",
        "@ionic/react-router": "^8.0.0",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "class-transformer": "^0.5.1",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "i18next": "^23.11.2",
        "i18next-browser-languagedetector": "^7.2.1",
        "inversify": "^6.0.2",
        "ionicons": "^7.0.0",
        "jeep-sqlite": "^2.7.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^5.3.4",
        "react-router-dom": "^5.3.4",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.20",
        "vue-auto-translate": "^1.1.13"
    },
    "devDependencies": {
        "@capacitor/cli": "6.0.0",
        "@testing-library/dom": ">=7.21.4",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.5.12",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@vitejs/plugin-legacy": "^5.0.0",
        "@vitejs/plugin-react": "^4.0.1",
        "autoprefixer": "^10.4.19",
        "cypress": "^13.5.0",
        "eslint": "^8.35.0",
        "eslint-plugin-react": "^7.32.2",
        "jsdom": "^22.1.0",
        "postcss": "^8.4.38",
        "prettier": "3.2.5",
        "sass": "^1.75.0",
        "tailwindcss": "^3.4.3",
        "terser": "^5.4.0",
        "typescript": "^5.1.6",
        "vite": "^5.0.0",
        "vite-plugin-pages": "^0.32.1",
        "vitest": "^0.34.6"
    },
    "description": "An Ionic project"
}